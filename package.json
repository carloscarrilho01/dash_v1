{
  "name": "n8n-chat-dashboard",
  "version": "1.0.0",
  "description": "Painel tipo WhatsApp para acompanhar conversas do agente IA do n8n",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon server/index.js",
    "dev:frontend": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "NODE_ENV=production node server/index.js",
    "test": "node test-conversation.js",
    "test:all": "node test-conversation.js all",
    "test:suporte": "node test-conversation.js suporte",
    "test:vendas": "node test-conversation.js vendas",
    "test:tecnico": "node test-conversation.js tecnico"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "date-fns": "^3.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.12"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "concurrently": "^8.2.2"
  }
}
